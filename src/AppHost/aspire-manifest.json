{
    "resources": {
        "apichatbot": {
            "type": "project.v0",
            "path": "../Services/ApiChatBot/ApiChatBot.csproj",
            "env": {
                "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
                "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
                "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
                "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true"
            },
            "bindings": {
                "http": {
                    "scheme": "http",
                    "protocol": "tcp",
                    "transport": "http"
                },
                "https": {
                    "scheme": "https",
                    "protocol": "tcp",
                    "transport": "http"
                }
            }
        },
        "chatbotweb": {
            "type": "dockerfile.v0",
            "path": "../Frontends/chatbotweb/Dockerfile",
            "context": "../Frontends/chatbotweb",
            "env": {
                "NODE_ENV": "production",
                "services__apichatbot__http__0": "{apichatbot.bindings.http.url}",
                "services__apichatbot__https__0": "{apichatbot.bindings.https.url}",
                "PORT": "{chatbotweb.bindings.http.targetPort}"
            },
            "bindings": {
                "http": {
                    "scheme": "http",
                    "protocol": "tcp",
                    "transport": "http",
                    "targetPort": 8000,
                    "external": true
                }
            }
        },
         "inspector": {
            "type": "dockerfile.v0",
            "path": "../Frontends/inspector/Dockerfile",
            "context": "../Frontends/inspector",
            "env": {
                "NODE_ENV": "production",
                "services__apichatbot__http__0": "{apichatbot.bindings.http.url}",
                "services__apichatbot__https__0": "{apichatbot.bindings.https.url}",
                "PORT": "{inspector.bindings.http.targetPort}"
            },
            "bindings": {
                "http": {
                    "scheme": "http",
                    "protocol": "tcp",
                    "transport": "http",
                    "targetPort": 8000,
                    "external": true
                }
            }
        }
    }
}